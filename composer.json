{
    "name": "shureban/laravel-prometheus",
    "description": "Laravel SDK for working with prometheus metrics",
    "keywords": [
        "prometheus",
        "metrics",
        "graphana",
        "counter",
        "gauge",
        "shureban"
    ],
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Shureban",
            "email": "Shureban@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "version": "0.1.0",
    "autoload": {
        "psr-4": {
            "Shureban\\LaravelPrometheus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shureban\\LaravelPrometheus\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "predis/predis": "^1.1",
        "myclabs/php-enum": "^1.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shureban\\LaravelPrometheus\\PrometheusServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
